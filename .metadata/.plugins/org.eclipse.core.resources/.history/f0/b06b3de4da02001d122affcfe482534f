
public class Main {

	public static void main(String[] args) {
		
		Cliente cl = new Cliente();
		cl.setNome("Jonas");

		ContaCorrente cc = new ContaCorrente(cl);
		cc.depositar(200);
		ContaPoupança cp = new ContaPoupança(cl); 
		cc.transferir(100, cp);
		
		cc.imprimirExtrato();
		cp.imprimirExtrato();
	}

}
